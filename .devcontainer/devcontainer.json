{
  "name": "Propulsor Intelligence",
  "image": "mcr.microsoft.com/devcontainers/python:3.10",
  "postCreateCommand": "pip install -r requirements.txt && streamlit run app.py --server.headless true",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "ms-python.autopep8",
        "ms-toolsai.jupyter-keymap",
        "ms-toolsai.jupyter-renderers",
        "MS-CEINTL.vscode-language-pack-pt-BR",
        "BroadcomMFD.debugger-for-mainframe"
      ]
    }
  }
}
